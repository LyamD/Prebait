<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
                name="smallCatchViewModel"
                type="com.prebait.thefishingsocialnetwork.session.catchs.smallCatchList.SmallCatchViewModel"
        />
    </data>
    <LinearLayout
            android:layout_margin="@dimen/margin_short"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".session.catchs.smallCatchList.SmallCatchFragment"
    >


        <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recycler_small_fish_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        />

    </LinearLayout>
</layout>